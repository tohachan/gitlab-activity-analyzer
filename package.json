{
  "name": "gitlab-activity-analyzer",
  "version": "0.1.0",
  "private": true,
  "author": "tohachan",
  "type": "module",
  "engines": {
    "node": ">=22.11.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "dev": "node server.cjs",
    "build": "vite build",
    "start": "NODE_ENV=production node server.cjs",
    "preview": "vite preview",
    "serve": "vite preview",
    "biome:check": "biome check",
    "biome:fix": "pnpm biome:check --write",
    "biome:ci": "biome ci",
    "typegen": "react-router typegen",
    "type:check": "react-router typegen && tsc",
    "turbo": "pnpm dlx turbo@2.1.2",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage run",
    "check:turbo": "pnpm turbo biome:check type:check test",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 commit-and-tag-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky",
    "knip": "knip",
    "pull-stats": "node ./tools/pull-gitlab-stats.cjs"
  },
  "dependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/types": "^19.5.0",
    "@fontsource-variable/plus-jakarta-sans": "^5.1.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@react-router/node": "^7.1.3",
    "@tailwindcss/vite": "^4.0.0",
    "@theme-toggles/react": "^4.1.0",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "express": "^4.18.2",
    "isbot": "^5.1.21",
    "lucide-react": "^0.473.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.1.3",
    "recharts": "^2.15.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/config-conventional": "^19.6.0",
    "@react-router/dev": "^7.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.8",
    "@types/react": "19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.0.3",
    "@vitest/ui": "^3.0.3",
    "commit-and-tag-version": "^12.5.0",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "knip": "^5.43.1",
    "lint-staged": "^15.4.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^4.0.0",
    "typescript": "5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.3"
  }
}
